{
	"arrowParens": "always",
	"bracketSpacing": true,
	"cursorOffset": -1,
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "css",
	"insertPragma": false,
	"jsxBracketSameLine": false,
	"jsxSingleQuote": false,
	"printWidth": 100,
	"proseWrap": "always",
	"quoteProps": "as-needed",
	"rangeStart": 0,
	"requirePragma": false,
	"semi": true,
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "none",
	"useTabs": true,
	"overrides": [
		{
			"files": "*.mdx",
			"options": {
				"printWidth": 78
			}
		}
	]
}
